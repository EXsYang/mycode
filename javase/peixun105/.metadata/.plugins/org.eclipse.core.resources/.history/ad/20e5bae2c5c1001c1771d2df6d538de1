package com.atguigu.review;

public class test1 {
	public static void main(String[] args) {
		
		//使用冒泡排序
		int[] arr = new int[]{34,5,22,-98,6,-76,0,-3};
		
		for(int i = 0;i < arr.length-1;i++) {//控制循环的轮数
			for(int j = 0;j<i-1;j++) {//控制排序次数
				if(arr[j]>arr[j+1]) {
					int temp = arr[j];
					arr[j] = arr[j+1];
					arr[j+1] = temp;
					
					
					
				}
				
				
				
				
				
				
			}
			
			
			
			
		}
		
		
		
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
}
