package com.atguigu.com;

public class Person  {//method

	public static void main(String[] args) {
		Son s = new Son();
		s.b();
	}
	private int age;
	
	public void a(){
		System.out.println("父类的a方法");
	}

}
class Son extends Person{
	private int age;
	public void a(){//Void是变量a的无效类型
		System.out.println("子类的a方法");
	}
	public void b() {
		a();
	}
	
}