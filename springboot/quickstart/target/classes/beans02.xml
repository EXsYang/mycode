<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--配置了 Monster bean monster03 不会注入到springboot的ioc容器中
    默认情况下 下面配置的bean
    ConfigurableApplicationContext ioc =
                SpringApplication.run(MainApp.class, args);
    -->
    <bean id="monster04" class="com.hspedu.springboot.bean.Monster">
        <property name="name" value="狐狸精~"></property>
        <property name="age" value="9999"></property>
        <property name="skill" value="美人计~"></property>
        <property name="id" value="2000"></property>
    </bean>
</beans>